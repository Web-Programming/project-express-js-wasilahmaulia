<% layout('layout') %>

<div class="container mt-5">
  <h1 class="text-center"><% title %></h1>
  <div class="row">
    <% product.forEach(product => { %>
    <div class="col-md-4 mb-4">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title"><%= product.name %></h4>
          <p class="card-text"><%=product.description %></p>
        </div>
      </div>
    </div>
    <% }); %>
  </div>
</div>
